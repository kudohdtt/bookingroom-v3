<?xml version="1.0" encoding="utf-8"?><!DOCTYPE androidx.constraintlayout.widget.ConstraintLayout>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".timphong.timphong"
    android:background="@drawable/backgroundtest"
    android:scaleType="centerCrop">


    <FrameLayout
        android:id="@+id/frameLayoutCheckin2"
        android:layout_width="412dp"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/backgroundchinh">

        <View
            android:id="@+id/line_2"
            android:layout_width="match_parent"
            android:layout_marginTop="2dp"
            android:layout_height="2dp"
            android:background="#4D787777"
            app:layout_constraintTop_toTopOf="@+id/frameLayoutCheckin2" />

        <View
            android:id="@+id/line_1"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#4D131313"
            app:layout_constraintTop_toTopOf="@+id/frameLayoutCheckin2" />
        <View
            android:id="@+id/line_3"
            android:layout_width="140dp"
            android:layout_marginLeft="140dp"
            android:layout_height="2dp"
            android:background="#80131313"
            android:layout_marginTop="218dp"
            app:layout_constraintTop_toTopOf="@+id/frameLayoutCheckin2" />
        <View
            android:id="@+id/line_4"
            android:layout_width="290dp"
            android:layout_marginLeft="15dp"
            android:layout_height="2dp"
            android:background="#73131313"
            android:layout_marginTop="345dp"
            app:layout_constraintTop_toTopOf="@+id/frameLayoutCheckin2" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayoutCheckin3"
        android:layout_width="412dp"
        android:layout_height="360dp"
        android:layout_marginTop="400dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/backgroundtest2"
        >
    </FrameLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="350dp"
        android:text="Yêu cầu đặt phòng"
        android:paddingLeft="15dp"
        android:textColor="#592A06"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="350dp"/>

    <TextView
        android:id="@+id/textViewChild2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="10dp"
        android:fontFamily="serif"
        android:text="@string/children"
        android:textColor="#592A06"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toStartOf="@+id/editChild2"
        app:layout_constraintTop_toBottomOf="@+id/textViewCO2" />



    <Button
        android:id="@+id/buttonSearch2"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="150dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="36dp"
        android:textColor="#000000"
        android:background="@drawable/buttontest"
        android:fontFamily="sans-serif-condensed-medium"
        android:gravity="center"
        android:linksClickable="false"
        android:textSize="20dp"
        android:text="@string/search"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="parent"
        tools:targetApi="p" />

    <TextView
        android:id="@+id/textViewCheckin2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="78dp"
        android:fontFamily="serif"
        android:text="@string/check_in"
        android:textAllCaps="false"
        android:textColor="#592A06"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textViewCheckout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="78dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="serif"
        android:text="@string/check_out"
        android:textColor="#592A06"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textViewAdult2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="14dp"
        android:fontFamily="serif"
        android:text="@string/adults"
        android:textColor="#592A06"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toStartOf="@+id/editAdult2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewCI2" />

    <EditText
        android:id="@+id/editAdult2"
        android:layout_width="39dp"
        android:layout_height="38dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/inputtest"
        android:hint="0"
        android:importantForAutofill="no"
        android:inputType="number"
        android:paddingLeft="20dp"
        android:layout_marginLeft="8dp"
        android:singleLine="true"
        android:textAlignment="textStart"
        android:textColor="#330000"
        app:layout_constraintStart_toEndOf="@+id/textViewAdult2"
        app:layout_constraintTop_toBottomOf="@+id/textViewCI2" />

    <EditText
        android:id="@+id/editChild2"
        android:layout_width="40dp"
        android:layout_height="38dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="60dp"
        android:autofillHints=""
        android:background="@drawable/inputtest"
        android:hint="0"
        android:inputType="number"
        android:paddingLeft="20dp"
        android:singleLine="true"
        android:textAlignment="textStart"
        android:textColor="#330000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewCO2" />


    <EditText
        android:id="@+id/textViewCI2"
        android:layout_width="133dp"
        android:layout_height="38dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="2dp"
        android:background="@drawable/inputtest"
        android:focusable="false"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewCheckin2" />

    <EditText
        android:id="@+id/textViewCO2"
        android:layout_width="134dp"
        android:layout_height="38dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="56dp"
        android:background="@drawable/inputtest"
        android:focusable="false"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewCheckout2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="50dp"
        app:layout_constraintEnd_toStartOf="@+id/textViewCO2"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/ic_calendar2_foreground" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="66dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/ic_calendar2_foreground" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:fontFamily="@font/dancing_script_bold"
        android:text="Royal Hotel"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#ffd700"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="410dp"
        android:layout_height="200dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/royallhotel"
        android:background="@android:drawable/dialog_holo_light_frame"
        android:scaleType="centerCrop"
        />

    <ImageView
        android:id="@+id/chananh"
        android:layout_width="match_parent"
        android:layout_height="142dp"
        android:layout_marginTop="185dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/ic_chananh"

        />

    <TextView
        android:id="@+id/ERcheckin2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/editAdult2"
        app:layout_constraintEnd_toEndOf="@+id/textViewCI2"
        app:layout_constraintStart_toStartOf="@+id/textViewCI2"
        app:layout_constraintTop_toBottomOf="@+id/textViewCI2" />

    <TextView
        android:id="@+id/ERcheckout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/editChild2"
        app:layout_constraintEnd_toEndOf="@+id/textViewCO2"
        app:layout_constraintStart_toStartOf="@+id/textViewCO2"
        app:layout_constraintTop_toBottomOf="@+id/textViewCO2" />

    <TextView
        android:id="@+id/ERadults2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/editAdult2"
        app:layout_constraintStart_toStartOf="@+id/editAdult2"
        app:layout_constraintTop_toBottomOf="@+id/editAdult2" />

    <TextView
        android:id="@+id/ERchildrent2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/editChild2"
        app:layout_constraintStart_toStartOf="@+id/editChild2"
        app:layout_constraintTop_toBottomOf="@+id/editChild2" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="52dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />


</androidx.constraintlayout.widget.ConstraintLayout>